{
  "name": "react-material-iconic-font",
  "version": "2.0.0",
  "description": "Material Iconic Font in React components",
  "main": "./dist/cjs.js",
  "browser": "./dist/umd.js",
  "module": "./src/index.js",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint .",
    "build": "webpack -p",
